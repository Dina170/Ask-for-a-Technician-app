<h1>All Technicians</h1>
<a href="/technicians/new">Add New Technician</a>
<ul>
    <% technicians.forEach(t=> { %>
        <li>
            <h3>
                <%= t.mainTitle %>
            </h3>
            <% if (t.jobTechnicianPhoto) { %>
                <img src="/uploads/technicians/<%= t.jobTechnicianPhoto %>" alt="Technician Photo" width="150">
                <% } %>
                    <div>
                        <a href="/technicians/<%= t._id %>">View</a>
                        <a href="/technicians/<%= t._id %>/edit">Edit</a>
                        <form action="/technicians/<%= t._id %>?_method=DELETE" method="POST" style="display:inline;">
                            <button type="submit" onclick="return confirm('Delete this technician?')">Delete</button>
                        </form>
                    </div>
        </li>
        <% }) %>
</ul>